\r\n        item.count++;\r\n        alreadyInCart = true;\r\n      }\r\n    });\r\n    if (!alreadyInCart) {\r\n      cartItems.push({ ...product, count: 1 }); //spread operator ...product means having fields of product\r\n    }\r\n    this.setState({ cartItems });\r\n    localStorage.setItem(\"cartItems\", JSON.stringify(cartItems));\r\n  };\r\n  render() {\r\n    return (\r\n      <Provider store={store}>\r\n        <div className=\"grid-container\">\r\n          <header>\r\n            <a href=\"/\">React Shopping</a>\r\n          </header>\r\n          <main>\r\n            <div className=\"content\">\r\n              <div className=\"main\">\r\n                <Filter></Filter>\r\n                <Products addToCart={this.addToCart}></Products>\r\n              </div>\r\n              <div className=\"sidebar\">\r\n                <Cart\r\n                  cartItems={this.state.cartItems}\r\n                  removeFromCart={this.removeFromCart}\r\n                  createOrder={this.createOrder}\r\n                ></Cart>\r\n              </div>\r\n            </div>\r\n          </main>\r\n          <footer>All right is reserverd.</footer>\r\n        </div>\r\n      </Provider>\r\n    );\r\n  }\r\n}\r\n\r\nexport default App;\r\n",["45","46"],"D:\\React and Redux\\react-shopping-cart\\src\\components\\Products.js",[],"D:\\React and Redux\\react-shopping-cart\\src\\util.js",[],"D:\\React and Redux\\react-shopping-cart\\src\\components\\Filter.js",[],"D:\\React and Redux\\react-shopping-cart\\src\\index.js",[],"D:\\React and Redux\\react-shopping-cart\\src\\components\\Cart.js",[],"D:\\React and Redux\\react-shopping-cart\\src\\store.js",[],"D:\\React and Redux\\react-shopping-cart\\src\\reducers\\productReducers.js",[],"D:\\React and Redux\\react-shopping-cart\\src\\types.js",[],"D:\\React and Redux\\react-shopping-cart\\src\\actions\\productActions.js",[],{"ruleId":"47","severity":1,"message":"48","line":1,"column":17,"nodeType":"49","messageId":"50","endL